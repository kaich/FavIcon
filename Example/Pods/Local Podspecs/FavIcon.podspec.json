{
  "name": "FavIcon",
  "version": "3.0.6",
  "summary": "A library for downloading website icons",
  "homepage": "https://github.com/leonbreedt/FavIcon",
  "license": {
    "type": "Apache",
    "file": "LICENSE"
  },
  "authors": {
    "Leon Breedt": "leon@sector42.io"
  },
  "source": {
    "git": "https://github.com/leonbreedt/FavIcon.git",
    "tag": "3.0.6"
  },
  "source_files": "Sources/**/*.swift",
  "preserve_paths": [
    "Sources/Modules/*",
    "Support/*.sh"
  ],
  "platforms": {
    "ios": "12.0",
    "osx": "10.13",
    "tvos": "12.0",
    "watchos": "4.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "5.0",
    "SWIFT_WHOLE_MODULE_OPTIMIZATION": "YES",
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "SWIFT_INCLUDE_PATHS": "$(PODS_ROOT)/../../Sources/Modules",
    "CLANG_ENABLE_MODULES": "YES",
    "DEFINES_MODULE": "YES"
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2",
    "SWIFT_INCLUDE_PATHS": "$(PODS_ROOT)/../../Sources/Modules",
    "OTHER_LDFLAGS": "-lxml2"
  },
  "libraries": "xml2"
}
